import java.io.*;
import java.util.*;
public class Solution {

public static void main(String[] args) 
{
    Scanner sc=new Scanner(System.in);
    String A=sc.next();
    String str="";
    char ch;
    for(int i=0;i<A.length();i++)
    {
       ch=A.charAt(i);
       str=ch+str; 
    }
    if(A.equals(str))
       {
           System.out.println("Yes");
       }
       else
       {
           System.out.println("No");
       }

}
}
